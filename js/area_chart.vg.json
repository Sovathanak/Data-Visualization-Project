{
    "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
    "description": "Area chart showing weight of cars over time.",
    "width": 600, "height": 400,
    "data": {"url": "https://raw.githubusercontent.com/Sovathanak/Data-Visualization-Project/master/data/broad_age_world.csv"},
    "mark": {
        "type":"area",
        "tooltip":true,
        "interpolate": "monotone"
      },

    "encoding": {
      "x": {
        "field": "Year", "type": "temporal"
      },
      "y": {
        "field": "Population", 
        "type": "quantitative"
        
      },
      "color": {
        "field": "Age range",
        "type": "nominal",
        "scale": {"range": ["#FF5E3C", "#71ADE7", "#4F5BFB", "#CA9EFF", "#6BCCC8"]}
      }
    }
  }